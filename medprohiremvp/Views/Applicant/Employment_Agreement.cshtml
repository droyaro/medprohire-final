@model string
@{
    ViewData["Title"] = "Employment Agreement";
}
<div class="new-heading new-heading-background ">
    <h2 class="">@ViewData["Title"]</h2>
</div>

<form asp-action="Employment_Agreement" id="Employment_Agreement" asp-controller="Applicant">
    <div class="usercontainer ">
        <div class="form-group text-left">
            <input type="submit" asp-action="Employment_Agreement_Sign" asp-controller="Applicant" class="btn btn-main animation smalltext" value=@Model />
        </div>
        <div class="background">
            <div asp-validation-summary="ModelOnly" class="text-danger text-left"></div>

            <div class="form-group">
                <iframe src=@Url.Action("Employment_Agreement_File","Applicant")></iframe>
            </div>
        </div>
    </div>
</form>

